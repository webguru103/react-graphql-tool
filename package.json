  "scripts": {
    "precommit": "yarn test && yarn lint && yarn flow",
    "start:devwatch": "yarn checkenv && CI=true yarn start:dev & yarn test:watch & yarn lint:watch",
    "start:prod": "yarn build && yarn serve:prod",
    "start:devwithmock": "yarn start:dev & yarn serve:mockserver & yarn serve",
    "start:dev": "yarn checkenv && webpack-dev-server --config build/webpack.development.js",
    "serve": "node server.js",
    "serve:mockserver": "babel-node mockServer",
    "serve:prod": "PRODUCTION=true STRIP_DATA_ID=true yarn serve",
    "build": "yarn checkenv && rimraf dist && STRIP_DATA_ID=true STATIC_SERVER='' webpack --config build/webpack.production.js",
    "build:e2e": "yarn checkenv && rimraf dist && STATIC_SERVER='' webpack --config build/webpack.production.js",
    "build:qa": "yarn checkenv && rimraf dist && STATIC_SERVER='' webpack --config build/webpack.qa.js",
    "checkenv": "node envCheck.js",
    "flow": "flow",
    "lint": "esw --ext .js,.jsx src tests",
    "lint:watch": "esw -w --ext .js,.jsx src tests",
    "test": "jest src tests",
    "test:watch": "jest --watch src tests",
    "test:coverage": "jest --coverage src tests",
    "test:e2e": "TEST_USER_PASSWORD=password TEST_USER_EMAIL=test@dealtap.ca FRONTEND_BASE_URL=http://localhost:10000 yarn run testcafe chrome e2e/**/*e2e.js -s testcafe/screenshots",
    "langbuild": "babel-node groupMessages.js"
  },
  "jest": {
    "setupFiles": [
      "./tests/jestSetup.js",
      "jest-canvas-mock"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg)$": "identity-obj-proxy",
      "\\.worker.*\\.js": "<rootDir>/tests/workerMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  }
